<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wxRobotApp="clr-namespace:wxRobotApp"
        x:Class="wxRobotApp.DetailsWindow"
        Width="400" Height="300"
        Title="WeChat Details"
       x:DataType="wxRobotApp:WeChatProcessInfo"
        >
    <StackPanel Margin="10" >
        <TextBlock Text="WeChat Details" FontWeight="Bold" Margin="0,0,0,10"/>
        <TextBlock Text="{Binding Pid}" Margin="0,5"/>
        <TextBlock Text="{Binding WeChatVersion}" Margin="0,5"/>
        <TextBlock Text="{Binding DllVersion}" Margin="0,5"/>
        <TextBlock Text="{Binding WeChatId}" Margin="0,5"/>
        <TextBlock Text="{Binding MessagePort}" Margin="0,5"/>
    </StackPanel>
</Window>